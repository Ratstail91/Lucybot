{
  "name": "lucybot",
  "version": "1.0.0",
  "description": "My own Discord bot",
  "main": "lucy.js",
  "author": "Kayne Ruse",
  "scripts": {
    "start": "forever -o forever.log -e error.log start lucy.js",
